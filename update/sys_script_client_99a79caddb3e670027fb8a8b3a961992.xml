        <name>Show/hide default fields</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	
	g_form.elements.forEach(function (cv) {
		if (cv.fieldName.startsWith('default_')) {
			if (cv.fieldName === 'default_' + newValue) {
				g_form.setVisible('default_' + newValue, true);
			} else {
				g_form.setVisible(cv.fieldName, false);
			}
		}
	});
}]]></script>
